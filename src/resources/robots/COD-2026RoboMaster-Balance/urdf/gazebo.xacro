<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
<!-- Gazebo配置文件 - 包含所有Gazebo特定的配置 -->

<!-- Gazebo材质定义 - 匹配MuJoCo中的颜色 -->
<gazebo reference="base_link">
  <material>Gazebo/Tertiary</material>
</gazebo>

<gazebo reference="Left_front_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Left_front_child1_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Left_front_child2_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Left_front_child3_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Left_rear_link">
  <material>Gazebo/Grey</material>
</gazebo>

<gazebo reference="Left_rear_child1_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Left_Wheel_link">
  <material>Gazebo/Grey</material>
</gazebo>

<gazebo reference="Right_front_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Right_front_child1_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Right_front_child2_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Right_front_child3_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Right_rear_link">
  <material>Gazebo/Grey</material>
</gazebo>

<gazebo reference="Right_rear_child1_link">
  <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="Right_Wheel_link">
  <material>Gazebo/Grey</material>
</gazebo>

<!-- Gazebo标签用于支持闭链机构仿真 -->
<gazebo>
  <!-- 左侧五连杆闭环约束1 连接Left_front_child2_link和Left_rear_link -->
  <joint name='Left_loop1_joint' type='revolute'>
    <pose relative_to='Left_rear_link'>-0.1005 -0.025 -0.0035 0 0 0</pose>
    <parent>Left_rear_link</parent>
    <child>Left_front_child2_link</child>
    <axis>
      <xyz>0 1 0</xyz>
      <limit>
        <lower>0</lower>
        <upper>0.98</upper>
        <effort>0</effort>
        <velocity>0</velocity>
      </limit>
      <dynamics>
        <damping>0.1</damping>
        <friction>0.01</friction>
      </dynamics>
    </axis>
  </joint>

  <!-- 左侧五连杆闭环约束2 连接Left_front_child3_link和Left_rear_child1_link -->
  <joint name='Left_loop2_joint' type='revolute'>
    <pose relative_to='Left_rear_child1_link'>-0.038 0 0.05 0 0 0</pose>
    <parent>Left_rear_child1_link</parent>
    <child>Left_front_child3_link</child>
    <axis>
      <xyz>0 1 0</xyz>
      <limit>
        <lower>-1.3</lower>
        <upper>0</upper>
        <effort>0</effort>
        <velocity>0</velocity>
      </limit>
      <dynamics>
        <damping>0.1</damping>
        <friction>0.01</friction>
      </dynamics>
    </axis>
  </joint>

  <!-- 右侧五连杆闭环约束1 连接Right_front_child2_link和Right_rear_link -->
  <joint name='Right_loop1_joint' type='revolute'>
    <pose relative_to='Right_rear_link'>-0.1 0.0225 -0.008 0 0 0</pose>
    <parent>Right_rear_link</parent>
    <child>Right_front_child2_link</child>
    <axis>
      <xyz>0 -1 0</xyz>
      <limit>
        <lower>-0.98</lower>
        <upper>0</upper>
        <effort>0</effort>
        <velocity>0</velocity>
      </limit>
      <dynamics>
        <damping>0.1</damping>
        <friction>0.01</friction>
      </dynamics>
    </axis>
  </joint>

  <!-- 右侧五连杆闭环约束2 连接Right_front_child3_link和Right_rear_child1_link -->
  <joint name='Right_loop2_joint' type='revolute'>
    <pose relative_to='Right_rear_child1_link'>-0.035 0 0.05 0 0 0</pose>
    <parent>Right_rear_child1_link</parent>
    <child>Right_front_child3_link</child>
    <axis>
      <xyz>0 -1 0</xyz>
      <limit>
        <lower>-1.3</lower>
        <upper>0</upper>
        <effort>0</effort>
        <velocity>0</velocity>
      </limit>
      <dynamics>
        <damping>0.1</damping>
        <friction>0.01</friction>
      </dynamics>
    </axis>
  </joint>
</gazebo>

</robot>